{
  "name": "entity_generator_g3",
  "type": "module",
  "version": "0.15.0",
  "packageManager": "pnpm@10.14.0",
  "description": "It is an entity generator for G3 Framework.",
  "author": "Zhang Junrong <http:https://github.com/Avitori73>",
  "license": "MIT",
  "homepage": "https://github.com/Avitori73/entity_generator_g3#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avitori73/entity_generator_g3.git"
  },
  "bugs": "https://github.com/Avitori73/entity_generator_g3/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "egg": "bin/egg.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "publish": "pnpm publish",
    "start": "tsx src/index.ts",
    "cli": "tsx src/commands/egg.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.0",
    "@antfu/ni": "^25.0.0",
    "@antfu/utils": "^9.2.0",
    "@clack/prompts": "^0.11.0",
    "@types/ini": "^4.1.1",
    "@types/lodash-es": "^4.17.12",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.17.1",
    "ansis": "^4.1.0",
    "bumpp": "^10.2.2",
    "change-case": "^5.4.4",
    "eslint": "^9.33.0",
    "ini": "^5.0.0",
    "lint-staged": "^16.1.5",
    "lodash-es": "^4.17.21",
    "minimist": "^1.2.8",
    "pgsql-ast-parser": "^12.0.1",
    "pnpm": "^10.14.0",
    "prettier": "^3.6.2",
    "prettier-plugin-java": "^2.7.4",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.13.1",
    "tinyglobby": "^0.2.14",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json}": "eslint --fix"
  }
}
