{
  "name": "entity_generator_g3",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@10.5.2",
  "description": "It is an entity generator for G3 Framework.",
  "author": "Zhang Junrong <http:https://github.com/Avitori73>",
  "license": "MIT",
  "homepage": "https://github.com/Avitori73/entity_generator_g3#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avitori73/entity_generator_g3.git"
  },
  "bugs": "https://github.com/Avitori73/entity_generator_g3/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "egg": "bin/egg.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "cli": "tsx src/commands/egg.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^8.1.0",
    "@clack/prompts": "^0.10.0",
    "@types/ini": "^4.1.1",
    "@types/node": "^22.13.1",
    "ansis": "^3.17.0",
    "bumpp": "^10.0.2",
    "change-case": "^5.4.4",
    "eslint": "^9.20.0",
    "ini": "^5.0.0",
    "lint-staged": "^15.4.3",
    "pgsql-ast-parser": "^12.0.1",
    "pnpm": "^10.2.1",
    "prettier": "^3.5.3",
    "prettier-plugin-java": "^2.6.7",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tinyglobby": "^0.2.12",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
